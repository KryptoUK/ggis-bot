{
    // You may need to remove comments for the json to parse correctly
    "botname": "ggis",
    "botnameproper": "Ggis",
    "token": "Discord OAuth token for the bot goes here!",
    "prefix": "!",
    "commandGroups": [
        {"code": ["debug","help"], "name":"Debug & Help", "description": "Help menus & debug"},
        {"code": ["useful"],"name":"Useful", "description": "Commands that do useful things"},
        {"code": ["random","rng"],"name":"Random", "description": "Random, fun commands"},
        {"code": ["memes"], "name":"Memes", "description": "Memes & dreams"}
    ],
    "eventHandlers": ["streamlinkHandler", "lfgHandler", "pollHandler", "spoilerHandler"],
    "functionsToReload": ["streamlink", "lfg", "poll", "spoiler"],
    "reloaders": ["commandsReload", "lfgReload"],
    "guilds": [], // "<bot will add a list of server ID's as it joins them in here>"
    "masterID": "Your User ID (bot owner)",
    "mainguild": "Main server ID, for guildOnly commands",
    "mainchannel": "Main channel in main guild",
    "testguild": "Test server ID, where most if not all testing takes place",
    // Whether certain GuildOnly features are enabled or not
    "rules": {
        "enable": true,
        "sailormoon": true,
        "autoreact": {
            "enable": true,
            "atmentions": true,
            "txtmentions": true
        },
        "extended_emoji": {
            "enable": true,
            "edit": true,
            "embed": true
        },
        "memes": true
    },
    "fortune": {
        "token": "IMGUR API Client Token",
        "album": "Album to look thru",
        "amount": 0 // Stores # of images here
    },
    "streamlink": {
        "default_setting": true,
        "enable": true,
        "timer": 60, //60 minutes
        "split_value": 10 //shows 10 user configs per message with StreamLink status cmd
    },
    "lfg": {
        //ttl = time to live in minutes
        "ttl_default": 15,
        "ttl_warning": true, // warn when longer lfg requests are running out of time?
        "ttl_max": 180,
        "ttl_threshold": 30,
        "max_party_size": 32,
        "split_value": 25 // where to split the lfg list
    },
    "squad": {
        "server_max": 25
    },
    "poll": {
        "divider": "|" // character which divides poll options
    }
}